# Configuración Git LFS para imágenes originales
_posts_images_original/**/*.jpg filter=lfs diff=lfs merge=lfs -text
_posts_images_original/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
_posts_images_original/**/*.png filter=lfs diff=lfs merge=lfs -text
_posts_images_original/**/*.gif filter=lfs diff=lfs merge=lfs -text
_posts_images_original/**/*.webp filter=lfs diff=lfs merge=lfs -text 